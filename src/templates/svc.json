{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "{{name}}"
  },
  "spec": {
    "type": "{{type}}",
    "selector": {
      "app": "{{name}}"
    },
    "ports": [
      {
        "port": "{{port}}",
        "protocol": "{{protocol}}",
        "targetPort": "{{targetPort}}"
      }
    ]
  }
}
